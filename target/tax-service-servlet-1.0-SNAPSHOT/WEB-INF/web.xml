    <context-param>
        <param-name>name</param-name>
        <param-value>Arthur</param-value>
    </context-param>

    <servlet>
        <servlet-name>MainServlet</servlet-name>
        <servlet-class>com.example.taxserviceservlet.web.controller.MainServlet</servlet-class>
